<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="0.5dip" android:color="@color/toggle_line_color" />
            <solid android:color="@color/line_press_color" />
            <corners android:radius="20.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="0.5dip" android:color="@color/toggle_line_color" />
            <solid android:color="@color/text_color_white_95" />
            <corners android:radius="20.0dip" />
        </shape>
    </item>
</selector>