<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:miui="http://schemas.android.com/apk/res/com.miui.securitycenter">
    <PreferenceCategory android:title="@string/st_msg_stranger_group" android:key="msg_stranger_group">
        <com.miui.antispam.ui.view.LabelPreference android:title="@string/st_antispam_title_anoy_sms" android:key="key_msg_stranger" miui:clickable="true" />
        <com.miui.antispam.ui.view.LabelPreference android:title="@string/st_antispam_title_service_sms" android:key="key_msg_notification" miui:clickable="true" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/st_msg_other_group" android:key="other_settings_group">
        <CheckBoxPreference android:persistent="false" android:title="@string/st_msg_intercept_contacts" android:key="key_msg_contacts" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/st_msg_keyword_group" android:key="keyword_group">
        <com.miui.antispam.ui.view.LabelPreference android:title="@string/st_msg_keyword_black" android:key="key_msg_keyword_black" miui:clickable="true" />
        <com.miui.antispam.ui.view.LabelPreference android:title="@string/st_msg_keyword_white" android:key="key_msg_keyword_white" miui:clickable="true" />
    </PreferenceCategory>
</PreferenceScreen>