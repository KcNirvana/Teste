<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen android:title="@string/infinity_display_settings"
  xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:settings="http://schemas.android.com/apk/res/com.android.settings">
    <CheckBoxPreference android:title="@string/notch_force_black_title" android:key="notch_force_black" android:summary="@string/notch_force_black_desc" />
    <PreferenceCategory android:title="@string/navigation_type_settings" android:key="navigation_type_category">
        <Preference android:layout="@layout/navigation_type_settings" />
    </PreferenceCategory>
    <PreferenceCategory android:key="navigation_setting_category">
        <CheckBoxPreference android:title="@string/navigation_guide_back_anim_title" android:key="navigation_back_anim" android:summary="@string/navigation_guide_back_anim_summary" />
        <CheckBoxPreference android:title="@string/fsg_mistake_touch_title" android:key="fsg_mistake_touch" android:summary="@string/fsg_mistake_touch_summary" />
        <CheckBoxPreference android:title="@string/switch_screen_button_order" android:key="switch_screen_button_order" android:summary="@string/switch_screen_button_order_desc" />
        <PreferenceScreen android:title="@string/screen_max_aspect_ratio_title" android:key="screen_max_aspect_ratio" android:summary="@string/screen_max_aspect_ratio_desc" android:fragment="com.android.settings.MaxAspectRatioSettings" />
    </PreferenceCategory>
    <PreferenceCategory android:title="@string/navigation_guide_settings" android:key="navigation_guide_category">
        <Preference android:icon="@drawable/navigation_bar_guide_home" android:layout="@layout/navigation_guide_item" android:title="@string/navigation_guide_home_title" android:key="navigation_guide_home" android:summary="@string/navigation_guide_home_summary" />
        <Preference android:icon="@drawable/navigation_bar_guide_recent" android:layout="@layout/navigation_guide_item" android:title="@string/navigation_guide_recent_title" android:key="navigation_guide_recent" android:summary="@string/navigation_guide_recent_summary" />
        <Preference android:icon="@drawable/navigation_bar_guide_back" android:layout="@layout/navigation_guide_item" android:title="@string/navigation_guide_back_title" android:key="navigation_guide_back" android:summary="@string/navigation_guide_back_summary" />
        <Preference android:icon="@drawable/navigation_bar_guide_app" android:layout="@layout/navigation_guide_item" android:title="@string/navigation_guide_app_title" android:key="navigation_guide_app" android:summary="@string/navigation_guide_app_summary" />
        <Preference android:icon="@drawable/navigation_bar_guide_appswitch" android:layout="@layout/navigation_guide_item_without_arrow" android:title="@string/navigation_guide_appswitch_title" android:selectable="false" android:key="navigation_guide_appswitch" android:summary="@string/navigation_guide_appswitch_summary" />
    </PreferenceCategory>
</PreferenceScreen>