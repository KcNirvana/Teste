        <TextView android:id="@id/button_4" android:background="@drawable/non_transparent_clickable_bottom_right_bg" android:visibility="gone" android:layout_width="0.0dip" android:layout_height="fill_parent" android:layout_marginLeft="1.0px" android:layout_weight="1.0" style="@style/ButtonStyle" />
